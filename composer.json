{
    "name": "dmlogic/community-contributions",
    "type": "library",
    "description": "Allow a small community to manage membership and fund contributions",
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.19"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DmLogic\\CommunityContributions\\Providers\\CommunityContributionsProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "DmLogic\\CommunityContributions\\": "src/",
            "DmLogic\\CommunityContributions\\Database\\Factories\\": "database/factories/",
            "DmLogic\\CommunityContributions\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
